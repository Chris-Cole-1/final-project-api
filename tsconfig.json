{
    "compilerOptions": {                        
      "target": "es6",                               
      "module": "commonjs",                           
      "outDir": "./dist",   
      "types": ["node", "express"],                          
      "rootDir": "./src",                             
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,      
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true                 
    },
    "exclude":[
      "./node_modules"
    ]
  }
